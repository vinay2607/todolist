<%-include('header');-%>
    <div class="box" id="heading"><h1 ><%= din %></h1> </div>
    <div class="box">
        <% items.forEach(function(h) { %>
            <form action="/delete" method="post">
            <div class="item">
                
                <input type="checkbox"  name="checkbox" value="<%= h._id %>" onChange="this.form.submit()" >
                <p><%=h.Work %></p>
            
            </div>
        </form>
        <% }); %>
            <form class="item" action="/" method="post" >
                <input type="text" name="add" placeholder="new item">
                <button type="submit" name="submit" value="<%= din %> ">+</button>
            </form>
            
    </div>
    <%- include('footer'); -%>   
     
